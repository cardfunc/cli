{
 "name": "@payfunc/cli-card",
 "version": "0.1.6",
 "description": "PayFunc command line interface for tokenizing.",
 "author": "PayFunc",
 "license": "MIT",
 "repository": {
  "type": "git",
  "url": "git+https://github.com/payfunc/cli-card"
 },
 "bugs": {
  "url": "https://github.com/payfunc/cli-card/issues"
 },
 "homepage": "https://github.com/payfunc/cli-card#readme",
 "private": false,
 "bin": {
  "payfunc-card": "dist/run.js"
 },
 "main": "dist/index.js",
 "typings": "dist/index.d.ts",
 "git": {
  "tagName": "v${version}"
 },
 "github": {
  "release": true
 },
 "jest": {
  "globals": {
   "ts-jest": {
    "tsConfig": "tsconfig.test.json"
   }
  },
  "testEnvironment": "node",
  "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
  "testPathIgnorePatterns": [
   "node_modules/",
   "dist/"
  ],
  "collectCoverageFrom": [
   "**/*.{ts,tsx,js,jsx}",
   "!**/node_modules/**",
   "!**/dist/**"
  ],
  "preset": "ts-jest",
  "testMatch": null
 },
 "scripts": {
  "dev": "watch 'npm run build' .",
  "lint": "eslint '**/*.{js,ts,tsx,jsx}'",
  "fix": "eslint '**/*.{js,ts,tsx,jsx}' --fix",
  "build": "tsc -p .",
  "pretest": "npm run build",
  "test": "node dist/run.js --server env test",
  "prerun": "npm run build",
  "run": "node dist/run.js",
  "prepare": "npm run build",
  "clean": "rm -rf dist node_modules coverage"
 },
 "dependencies": {
  "@payfunc/model-card": "0.1.12",
  "@payfunc/model": "0.2.39",
  "@payfunc/service-ch3d1": "0.0.29",
  "@payfunc/service-ch3d2": "0.0.32",
  "@payfunc/service-clearhaus": "payfunc/service-clearhaus#backend-worker",
  "@types/dotenv": "^8.2.0",
  "authly": "0.0.40",
  "dotenv": "^8.2.0",
  "gracely": "0.0.33",
  "node-fetch": "^2.6.1",
  "node-persist": "^3.1.0",
  "open": "^7.0.4",
  "paramly": "0.0.5"
 },
 "devDependencies": {
  "@types/form-urlencoded": "^2.0.1",
  "@types/jest": "^26.0.4",
  "@types/node": "^14.0.23",
  "@types/node-fetch": "^2.5.7",
  "@types/node-persist": "^3.1.0",
  "@types/open": "^6.2.1",
  "@typescript-eslint/eslint-plugin": "^3.10.1",
  "@typescript-eslint/parser": "^3.10.1",
  "babel-core": "^6.26.3",
  "babel-jest": "^26.1.0",
  "eslint": "^7.7.0",
  "eslint-plugin-prettierx": "github:nilssonemma/eslint-plugin-prettierx#master",
  "isoly": "0.0.15",
  "jest": "^26",
  "prettierx": "github:simonmika/prettierx#newline-before-clause",
  "ts-jest": "^26.1.3",
  "typescript": "^3.9.7",
  "watch": "^1.0.2"
 }
}
